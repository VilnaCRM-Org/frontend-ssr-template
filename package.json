{
  "name": "frontend-ssr-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export",
    "start": "next start",
    "git-hooks-install": "husky install",
    "storybook-start": "start-storybook -p 6006",
    "storybook-build": "build-storybook",
    "generate-ts-doc": "api-extractor run --local --verbose",
    "format": "prettier . --write",
    "lint:next": "next lint",
    "lint:tsc": "tsc",
    "lint:md": "markdownlint -i CHANGELOG.md **/*.md",
    "test:e2e": "cypress run --headless --browser chrome",
    "test:e2e-local": "cypress open",
    "test:unit": "jest --verbose",
    "test:memory-leak": "memlab run -v --scenario ./memlab/memoryLeakTest.js",
    "lighthouse:desktop": "next build && next export && lhci autorun --config=lighthouserc.desktop.js",
    "lighthouse:mobile": "next build && next export && lhci autorun --config=lighthouserc.mobile.js",
    "check-node-version": "node checkNodeVersion.js",
    "preinstall": "pnpm run check-node-version"
  },
  "dependencies": {
    "@apollo/client": "^3.11.8",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/material": "^5.16.7",
    "@sentry/node": "^7.119.0",
    "@sentry/react": "^7.119.0",
    "cypress": "^11.2.0",
    "dotenv": "^16.4.5",
    "graphql": "^16.9.0",
    "i18next": "^22.5.1",
    "jest": "^29.7.0",
    "jsdoc": "^4.0.3",
    "next": "^13.5.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^12.3.1",
    "reflect-metadata": "^0.2.2",
    "require-from-string": "^2.0.2",
    "storybook": "^8.3.5",
    "tsyringe": "^4.8.0",
    "web-vitals": "^3.5.2",
    "webpack": "^5.95.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.7",
    "@commitlint/cli": "^17.8.1",
    "@commitlint/config-conventional": "^17.8.1",
    "@lhci/cli": "^0.12.0",
    "@microsoft/api-extractor": "^7.47.9",
    "@next/eslint-plugin-next": "^13.5.7",
    "@storybook/addon-actions": "^8.3.5",
    "@storybook/addon-essentials": "^8.3.5",
    "@storybook/addon-interactions": "^8.3.5",
    "@storybook/addon-links": "^8.3.5",
    "@storybook/builder-webpack5": "^8.3.5",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^8.3.5",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.5.13",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "babel-loader": "^9.2.1",
    "eslint": "^8.28.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-next": "^14.2.14",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-cypress": "^2.15.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-testing-library": "^5.11.1",
    "husky": "^8.0.3",
    "markdownlint-cli": "^0.34.0",
    "memlab": "^1.1.55",
    "prettier": "^3.3.3",
    "semver": "^7.6.3",
    "ts-jest": "^29.2.5",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "engineStrict": true
}
