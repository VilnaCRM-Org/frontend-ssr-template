{
  "name": "frontend-ssr-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next build && npx serve@latest out",
    "git-hooks-install": "husky install",
    "storybook-start": "start-storybook -p 6006",
    "storybook-build": "build-storybook",
    "generate-ts-doc": "api-extractor run --local --verbose",
    "format": "prettier . --write",
    "lint:next": "next lint",
    "lint:tsc": "tsc",
    "lint:md": "markdownlint -i CHANGELOG.md **/*.md",
    "test:e2e": "cypress run --headless --browser chrome",
    "test:e2e-local": "cypress open",
    "test:unit": "jest --verbose",
    "test:memory-leak": "memlab run -v --scenario ./memlab/memoryLeakTest.js",
    "lighthouse:desktop": "next build && lhci autorun --config=lighthouserc.desktop.js --collect.startServerCommand=\"npx serve@latest out\"",
    "lighthouse:mobile": "next build && lhci autorun --config=lighthouserc.mobile.js --collect.startServerCommand=\"npx serve@latest out\"",
    "check-node-version": "node checkNodeVersion.js",
    "preinstall": "pnpm run check-node-version"
  },
  "dependencies": {
    "@apollo/client": "^3.13.8",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/material": "^7.2.0",
    "@sentry/node": "^9.38.0",
    "@sentry/react": "^9.38.0",
    "dotenv": "^17.2.0",
    "graphql": "^16.11.0",
    "i18next": "^25.3.2",
    "jest": "^30.0.4",
    "jsdoc": "^4.0.4",
    "next": "^15.4.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-i18next": "^15.6.0",
    "reflect-metadata": "^0.2.2",
    "require-from-string": "^2.0.2",
    "storybook": "^9.0.16",
    "terser-webpack-plugin": "^5.3.14",
    "tsyringe": "^4.10.0",
    "web-vitals": "^5.0.3",
    "webpack": "^5.100.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@lhci/cli": "^0.15.1",
    "@microsoft/api-extractor": "^7.52.8",
    "@next/eslint-plugin-next": "^15.4.1",
    "@storybook/addon-actions": "^9.0.8",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-links": "^9.0.16",
    "@storybook/builder-webpack5": "^9.0.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^9.0.16",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.13",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "babel-loader": "^10.0.0",
    "eslint": "^9.31.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-next": "^15.4.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-cypress": "^5.1.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-storybook": "^9.0.16",
    "eslint-plugin-testing-library": "^7.6.0",
    "husky": "^9.1.7",
    "markdownlint-cli": "^0.45.0",
    "memlab": "^1.1.59",
    "prettier": "^3.6.2",
    "semver": "^7.7.2",
    "ts-jest": "^29.4.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "engineStrict": true
}
